<?xml version="1.0" encoding="UTF-8"?>
<channelml xmlns="http://morphml.org/channelml/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:meta="http://morphml.org/metadata/schema" xsi:schemaLocation="http://morphml.org/channelml/schema  http://www.morphml.org:8080/NeuroMLValidator/NeuroMLFiles/Schemata/v1.8.0/Level2/ChannelML_v1.8.0.xsd" units="Physiological Units">

    <meta:notes>A channel from Bhalla, U.S.and Bower, J.M. Exploring parameter space in detailed single neuron models:
    simulations of the mitral and granule cells of the olfactory bulb</meta:notes>

  
                                                                <!-- Using post v1.7.3 preferred ChannelML version
    <ion name="k" default_erev="-0.08" charge="1" role="PermeatedSubstance">
        <meta:notes>The ion which actually flows through the channel</meta:notes>
    </ion>
	<ion name="ca" charge="2" role="ModulatingSubstance">
        <meta:notes>The channel's conductance of K is dependent on this ion's concentration</meta:notes>
    </ion>-->














    
    
    <channel_type name="KCa3_ChannelML_new" density="yes">
        
        <status value="in_progress">
            <meta:issue>This ChannelML file is intended to replicate the GENESIS functionality of a tabchannel version of Kca_mit_usb </meta:issue>
            <meta:contributor>
                <meta:name>Simon O'Connor</meta:name>
            </meta:contributor>
        </status>
    
        <meta:notes>Calcium dependent K channel</meta:notes>
        
        <meta:publication>
		<meta:fullTitle>Bhalla, U.S.and Bower, J.M. Exploring parameter space in detailed single neuron models:
    simulations of the mitral and granule cells of the olfactory bulb. J Neurophysiol, Nov 1993; 69: 1948 - 1965</meta:fullTitle>
           <meta:pubmedRef>http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&amp;db=PubMed&amp;list_uids=7688798&amp;dopt=Abstract</meta:pubmedRef>
        </meta:publication>


        <meta:neuronDBref>
            <meta:modelName>K channels</meta:modelName>
            <meta:uri>http://senselab.med.yale.edu/senselab/NeuronDB/channelGene2.htm#table3</meta:uri>
        </meta:neuronDBref>

        
                                                                <!-- Using post v1.7.3 preferred ChannelML version-->














        
        <current_voltage_relation cond_law="ohmic" ion="k" default_gmax="3.6" default_erev="-90">
          
          <conc_dependence name="Calcium" ion="ca" charge="2" variable_name="ca_conc" min_conc="0" max_conc="1e-8"/>
       
          
          <gate name="m" instances="1">
            <closed_state id="m0"/>
            <open_state id="m" fraction="1"/>
            
	    <transition name="alpha" from="m0" to="m" expr_form="generic" expr="(exp ((v-65)/27))"/>
	    <transition name="beta" from="m" to="m0" expr_form="generic" expr="0.008"/>
            
          </gate>
	  <gate name="z" instances="1"> 
		  <closed_state id="z0"/>
		  <open_state id="z" fraction="1"/>
		  
		  <transition name="alpha" from="z0" to="z" expr_form="generic" expr="(500.0*(0.015 - (ca_conc*1e6)))/( (exp ((0.015 - (ca_conc*1e6))/0.0013)) -1)"/>
		  <transition name="beta" from="z" to="z0" expr_form="generic" expr="0.0021"/>
		  
          </gate>
        </current_voltage_relation>  
        
